function isOdd(number) {
    if (typeof number !== "number") {
        return false;
    }
    return Math.abs(number) % 2 === 1; 
}

console.log("1 is:" + isOdd(1));
console.log("2 is:" + isOdd(2));
console.log("3 is:" + isOdd(1.8));
console.log("4 is:" + isOdd("string"));
console.log("5 is:" + isOdd(-3));
